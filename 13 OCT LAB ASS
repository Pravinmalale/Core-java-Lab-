Q.2
import java.util.Scanner;
public class pattern
{
	public static void main(String args[])
	{
		Scanner scaner=new Scanner(System.in);
		int rows=scaner.nextInt();
		System.out.println(" priting the pattern");
		 for (int i=1;i<=rows;i++)
		 {
			 for (int j=1;j<=i;j++)
			 {
				 System.out.print(j +" ");
	
		}
		
		for (int k=i-1;k>=1;k--)
		{
			System.out.print(k + " ");
			
		}
		System.out.println();
		 }
		 
		 
	}
}
